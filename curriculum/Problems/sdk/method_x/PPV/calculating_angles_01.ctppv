            <Property name="order" value="25"/>
            <Property name="group" value="group16"/>
            <Property name="text" value="m&lt;latex&gt;\angle BPC&lt;/latex&gt; + m&lt;latex&gt;\angle DPA&lt;/latex&gt; + m&lt;latex&gt;\angle CPA&lt;/latex&gt; + m&lt;latex&gt;\angle BPD&lt;/latex&gt; = {{1}}°"/>
